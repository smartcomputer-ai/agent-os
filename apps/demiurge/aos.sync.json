{
  "air": {
    "dir": "air",
    "imports": [
      {
        "cargo": {
          "package": "aos-agent-sdk",
          "air_dir": "air/exports/session-contracts",
          "manifest_path": "../../Cargo.toml"
        }
      }
    ]
  },
  "build": {
    "reducer_dir": "reducer"
  },
  "modules": {
    "pull": false
  },
  "version": 1,
  "workspaces": [
    {
      "dir": "reducer",
      "ignore": [
        "target/",
        ".git/",
        ".aos/"
      ],
      "ref": "reducer"
    },
    {
      "dir": "tools",
      "ignore": [
        ".git/",
        ".aos/"
      ],
      "ref": "demiurge"
    }
  ]
}
