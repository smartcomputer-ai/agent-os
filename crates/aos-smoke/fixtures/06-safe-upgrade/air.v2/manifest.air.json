{
  "$kind": "manifest",
  "air_version": "1",
  "schemas": [
    {
      "name": "demo/SafeUpgradePc@1"
    },
    {
      "name": "demo/SafeUpgradeState@1"
    },
    {
      "name": "demo/SafeUpgradeStart@1"
    },
    {
      "name": "demo/SafeUpgradeEvent@1"
    },
    {
      "name": "sys/EffectReceiptEnvelope@1"
    },
    {
      "name": "sys/HttpRequestParams@1"
    },
    {
      "name": "sys/HttpRequestReceipt@1"
    }
  ],
  "modules": [
    {
      "name": "demo/SafeUpgrade@2"
    },
    {
      "name": "sys/CapEnforceHttpOut@1"
    }
  ],
  "caps": [
    {
      "name": "demo/http_fetch_cap@1"
    }
  ],
  "policies": [
    {
      "name": "demo/http-policy@2"
    }
  ],
  "defaults": {
    "policy": "demo/http-policy@2",
    "cap_grants": [
      {
        "name": "cap_http_fetch",
        "cap": "demo/http_fetch_cap@1",
        "params": {
          "record": {
            "hosts": {
              "set": [
                {
                  "text": "example.com"
                }
              ]
            },
            "methods": {
              "set": [
                {
                  "text": "GET"
                }
              ]
            }
          }
        }
      }
    ]
  },
  "routing": {
    "subscriptions": [
      {
        "event": "demo/SafeUpgradeEvent@1",
        "module": "demo/SafeUpgrade@2"
      }
    ]
  },
  "effects": [
    {
      "name": "sys/http.request@1"
    }
  ]
}
