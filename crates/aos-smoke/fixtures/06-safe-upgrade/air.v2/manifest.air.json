{
  "$kind": "manifest",
  "schemas": [
    {
      "name": "demo/SafeUpgradePc@1"
    },
    {
      "name": "demo/SafeUpgradeState@1"
    },
    {
      "name": "demo/SafeUpgradeStart@1"
    },
    {
      "name": "demo/SafeUpgradeNotifyComplete@1"
    },
    {
      "name": "demo/SafeUpgradeEvent@1"
    },
    {
      "name": "demo/UpgradeFetchRequest@1"
    },
    {
      "name": "demo/SafeUpgradeResult@1"
    },
    {
      "name": "sys/HttpRequestParams@1"
    },
    {
      "name": "sys/HttpRequestReceipt@1"
    }
  ],
  "modules": [
    {
      "name": "demo/SafeUpgrade@1"
    }
  ],
  "plans": [
    {
      "name": "demo/fetch_plan@2"
    }
  ],
  "caps": [
    {
      "name": "demo/http_fetch_cap@1"
    },
    {
      "name": "demo/http_followup_cap@1"
    }
  ],
  "policies": [
    {
      "name": "demo/http-policy@2"
    }
  ],
  "defaults": {
    "policy": "demo/http-policy@2",
    "cap_grants": [
      {
        "name": "cap_http_fetch",
        "cap": "demo/http_fetch_cap@1",
        "params": {
          "record": {
            "hosts": {
              "set": [
                {
                  "text": "example.com"
                }
              ]
            },
            "methods": {
              "set": [
                {
                  "text": "GET"
                }
              ]
            }
          }
        }
      },
      {
        "name": "cap_http_followup",
        "cap": "demo/http_followup_cap@1",
        "params": {
          "record": {
            "hosts": {
              "set": [
                {
                  "text": "example.com"
                }
              ]
            },
            "methods": {
              "set": [
                {
                  "text": "GET"
                }
              ]
            }
          }
        }
      }
    ]
  },
  "routing": {
    "events": [
      {
        "event": "demo/SafeUpgradeEvent@1",
        "reducer": "demo/SafeUpgrade@1"
      }
    ],
    "inboxes": []
  },
  "triggers": [
    {
      "event": "demo/UpgradeFetchRequest@1",
      "plan": "demo/fetch_plan@2",
      "correlate_by": "request_id"
    }
  ],
  "air_version": "1",
  "effects": [
    {
      "name": "sys/http.request@1"
    }
  ]
