    {
      "name": "sys/HttpRequestReceipt@1"
    },
    {
      "name": "sys/LlmGenerateParams@1"
    },
    {
      "name": "sys/LlmGenerateReceipt@1"
    },
    {
      "name": "sys/SecretRef@1"
    },
    {
      "name": "sys/TextOrSecretRef@1"
