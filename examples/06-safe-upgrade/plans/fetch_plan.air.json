[
  {
    "$kind": "defplan",
    "name": "demo/fetch_plan@1",
    "input": "demo/UpgradeFetchRequest@1",
    "output": "demo/SafeUpgradeResult@1",
    "steps": [
      {
        "id": "emit_primary",
        "op": "emit_effect",
        "kind": "http.request",
        "params": {
          "record": {
            "method": { "text": "GET" },
            "url": { "text": "https://example.com/data.json" },
            "idempotency_key": { "text": "safe-upgrade-v1-primary" },
            "headers": { "map": [] },
            "body_ref": { "const": { "null": {} } }
          }
        },
        "cap": "cap_http_fetch",
        "bind": { "effect_id_as": "primary_req" }
      },
      {
        "id": "await_primary",
        "op": "await_receipt",
        "for": { "ref": "@var:primary_req" },
        "bind": { "as": "primary_receipt" }
      },
      {
        "id": "capture_primary_status",
        "op": "assign",
        "expr": { "int": 200 },
        "bind": { "as": "primary_status" }
      },
      {
        "id": "raise_complete",
        "op": "raise_event",
        "reducer": "demo/SafeUpgrade@1",
        "event": {
          "variant": {
            "tag": "NotifyComplete",
            "value": {
              "record": {
                "primary_status": { "ref": "@var:primary_status" },
                "follow_status": { "int": -1 },
                "request_count": { "nat": 1 }
              }
            }
          }
        }
      },
      {
        "id": "finish",
        "op": "end",
        "result": {
          "record": {
            "primary_status": { "ref": "@var:primary_status" },
            "follow_status": { "int": -1 }
          }
        }
      }
    ],
    "edges": [
      { "from": "emit_primary", "to": "await_primary" },
      { "from": "await_primary", "to": "capture_primary_status" },
      { "from": "capture_primary_status", "to": "raise_complete" },
      { "from": "raise_complete", "to": "finish" }
    ],
    "required_caps": ["cap_http_fetch"],
    "allowed_effects": ["http.request"]
  }
]
